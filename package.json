{
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=!docusaurus",
    "build:docs": "turbo run build --filter=docusaurus",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "turbo run build --filter=docusaurus"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^8.0.0",
    "eslint-config-react-native-mlkit": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "name": "react-native-mlkit",
  "workspaces": [
    "apps/*",
    "modules/*",
    "packages/*"
  ],
  "packageManager": "yarn@3.6.1",
  "repository": "https://www.github.com/infinitered/react-native-mlkit"
}
